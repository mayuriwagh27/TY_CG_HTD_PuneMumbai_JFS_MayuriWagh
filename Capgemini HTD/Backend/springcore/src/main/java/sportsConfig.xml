<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- For MedicineBean -->
	<!-- for Map -->

	<bean id="sports"
		class="com.capgemini.springcore.annotation.bean.Sports">
		<property name="name" value="cricket"></property>
		<property name="totalPlayers" value="15"></property>
		<property name="teamInfo">
			<map>
				<entry key="India" value="DHONI"></entry>
				<entry key="Pakistan" value="Sarfaraz"></entry>
				<entry key="Australia" value="Aron finch"></entry>

			</map>
		</property>


	</bean>
</beans>
